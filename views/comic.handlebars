<h1>Daily Comics</h1>

<button id="retrieve">Get Today's Comics</button>

<h2>
    {{text}}
</h2>

{{#each comic}}

<div class="card">
    <div class="card-body">

        <div class="card-title">
            {{comic}}
        </div>

        <img class="card-img-bottom" src={{img}} alt={{comic}}>
    </div>
</div>
<br>

{{/each}}